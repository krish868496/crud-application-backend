{
  "version": 2,
  "builds": [
    { "src": "api/v1/blog.js", "use": "@vercel/node" },
    { "src": "api/index.js", "use": "@vercel/node" },
    { "src": "api/car.js", "use": "@vercel/node" }
  ],
  "routes": [
    { "src": "/api/v1/blog", "dest": "/api/v1/blog.js" },
    { "src": "/", "dest": "/api/index.js" },
    { "src": "/car", "dest": "/api/car.js" }
  ]
}
